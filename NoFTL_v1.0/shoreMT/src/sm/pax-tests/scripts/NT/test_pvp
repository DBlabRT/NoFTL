# Selectivity tests

# 1%
export EMON_COMMAND="file_scan -device_name e:/shore_volumes_pvp/disk_8num_2xl -page_layout 2 -T S1"
echo $EMON_COMMAND >> timer_pvp
echo $EMON_COMMAND
$EMON_COMMAND >> timer_pvp
echo "**********************" >> timer_pvp
